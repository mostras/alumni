<%= render 'shared/navbar' %>
<div class="main-wrapper">
  <h1>Les entreprises</h1>

  <div class="company-card-wrapper">
    <% @companies.each do |company| %>
      <%= link_to company_path(company) do %>
        <%= render 'shared/company-card', company: company %>
      <% end %>
    <% end %>
  </div>
</div>
